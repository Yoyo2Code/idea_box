<h1>Create an account</h1>

<%= flash.now[:error] %>
<%= flash[:notice] %>

<%= form_for(@user) do |t| %>
  <%= t.label :username %>
  <%= t.text_field :username %>
    </br>
      </br>
  <%= t.label :password %>
  <%= t.password_field :password %>
    </br>
      </br>
  <%= t.submit "Create Account" %>
<% end %>
